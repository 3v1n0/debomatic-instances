DISTRIBUTION=unstable
MIRRORSITE=http://ftp.debian.org/debian
COMPONENTS="main contrib non-free"
OTHERMIRROR="deb $MIRRORSITE $DISTRIBUTION $COMPONENTS | deb http://debomatic-powerpc.debian.net/debomatic/unstable unstable main"
DEBOOTSTRAP=debootstrap
DEBOOTSTRAPOPTS=('--variant=buildd' '--keyring' '/usr/share/keyrings/debian-archive-keyring.gpg')
ALLOWUNTRUSTED=yes
