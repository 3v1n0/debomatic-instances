[default]
builder: cowbuilder
debootstrap: qemu-debootstrap
packagedir: /srv/debomatic-powerpc
configdir: /home/debomatic/config/distributions/powerpc
architecture: powerpc
pbuilderhooks: /home/debomatic/config/pbuilderhooks
maxbuilds: 1
inotify: 1
sleep: 60
logfile: /var/log/debomatic-powerpc.log
loglevel: info

[gpg]
gpg: 1
keyring: /home/debomatic/config/keyring/debomatic.gpg

[modules]
modules: 1
modulespath: /home/debomatic/debomatic/modules

[runtime]
distblacklist: oldstable
alwaysupdate: unstable experimental utopic
modulesblacklist: AutoPkgTest Blhc Lintian Mailer Piuparts
mapper = {'sid':'unstable',
          'testing-proposed-updates':'testing',
          'utopic-proposed':'utopic'}

[lintian]
lintopts: -i -I -E --pedantic

[mailer]
fromaddr: debomatic@debomatic.debian.net
smtphost: 172.17.11.200
smtpport: 25
authrequired: 0
smtpuser: user
smtppass: pass
success: /home/debomatic/debomatic/etc/debomatic/mailer/build_success.mail-template
failure: /home/debomatic/debomatic/etc/debomatic/mailer/build_failure.mail-template
lintlog: 0

[piuparts]
piupopts: --log-level=info

[repository]
gpgkey: 0xF4914DAB
pubring: /home/debomatic/config/keyring-private/pubring.gpg
secring: /home/debomatic/config/keyring-private/secring.gpg

[autopkgtest]
options = --no-built-binaries
logging = True
gpghome = /home/debomatic/config/keyring

[jsonlogger]
jsonfile = /var/log/debomatic-powerpc.json

[internals]
configversion: 013a
